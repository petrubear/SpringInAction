<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>
                %highlight(%d{HH:mm:ss.SSS} %-5level %logger{36} - %msg%n)
            </pattern>
        </encoder>
    </appender>
    <logger name="root" level="INFO"/>
    <logger name="emg.taco" level="DEBUG"/>
    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>